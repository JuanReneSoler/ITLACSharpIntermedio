

@model Models.Dtos.LogInRequest
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>
    .form-signin {
        width: 100%;
        max-width: 330px;
        padding: 15px;
        margin: auto;
    }
</style>


@using (Html.BeginForm("LogIn", "Account", FormMethod.Post, new { @class = "form-signin text-center" }))
{
    <img class="mb-4" src=" data:image/png;base64," alt="" width="72" height="72">
    <h1 class="h3 mb-3 font-weight-normal">Inicie Sesion</h1>
    @Html.AntiForgeryToken()
    @Html.ValidationSummary("", new { @class = "was-validated" })
    @Html.LabelFor(x => Model.UserName, new { @class = "sr-only", @placeholder = "Nombre de Usuario", @autofocus = "" })
    @Html.TextBoxFor(x => Model.UserName, new { @class = "form-control" })
    @Html.LabelFor(x => Model.Password, new { @class = "sr-only", @placeholder = "Contrasena" })
    @Html.TextBoxFor(x => Model.Password, new { @class = "form-control", @type = "password" })
    <div class="checkbox mb-3">
        <label>
            @*<input type="checkbox" value="remember-me"> Remember me*@
            @Html.CheckBoxFor(x=>x.RememberMe,new { }) @Html.LabelFor(x=>x.RememberMe,new { })
        </label>
    </div>
    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
    <p class="mt-5 mb-3 text-muted">Juan Soler (2013-1715)</p>
    <p class="text-muted" >Ian de Jesus (80864)</p>

