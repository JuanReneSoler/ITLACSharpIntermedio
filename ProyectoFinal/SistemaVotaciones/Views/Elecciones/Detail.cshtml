
@{
    Layout = "~/Views/Shared/Administrador.cshtml";
}
@model Models.Dtos.EleccionesRequest

<div class="form-row">
    <div class="col-md-12">
        @Html.HiddenFor(x=>Model.Id,new{})
    </div>
    <div class="col-md-12 mb-3">
        @Html.LabelFor(x=>Model.Nombre,new{})
        @Html.TextBoxFor(x=>Model.Nombre,new{@class="form-control", @readonly=""})
    </div>
    <div class="col-md-12 mb-3">
        @Html.LabelFor(x=>Model.Fecha,new{})
        @Html.TextBoxFor(x=>Model.Fecha,"{0:yyyy-MM-dd}", new{@type="date", @class="form-control", @readonly=""})
    </div>
    <div class="col-md-12 mb-3">
        <a href="/Elecciones/Index" class="btn btn-danger">Cancelar</a>
    </div>
</div>


